footer
	section#links
		.wrapper.cs-wrapper
			.wrapper.m-3-col
				.international
					h3 International
					ul
						each url in urls
							li
								a(href=url.link, class=url.class, title=url.title)
									i(class="icon icon-"+url.class)
									= url.title
				.ipricegroup
					h3 iPrice group
					ul
						each group in groups
							li
								a(href=group.link, class=group.class, title=group.title, target="_blank")= group.title
				.none-l
				.social
					h3 Social
					ul
						each social in socials
							li
								a(href=social.link, title=social.title, target="_blank") 
									i(class="icon icon-"+social.class)
									= social.title
